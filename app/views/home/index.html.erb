<!doctype html>
<html lang="en">
<head>
	<title>Unipload</title>
	<meta charset="utf-8">
  	<meta name="viewport" content="width=device-width, initial-scale=1">
  	<link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
  	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
  	<script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
</head>
<body>
<!-- BODY -->

<p>PAWEl</p>
<div class="container" id="main">
		<div class="row">
			<div class="col-lg-2 col-lg-offset-5" id="test">
				<%= image_tag('logo.png', class: 'img-responsive', alt: 'unipload_logo')%>
			</div>
		</div>
		<div class="row" id="unipload">
			<div class="col-lg-6 col-lg-offset-3">
					<%= image_tag('unipload.png', class: 'img-responsive', alt: 'unipload_logo')%>
			</div>
		</div>
		<div class="row" id="signInButton">
			<div class="col-lg-2 col-lg-offset-2">
				<a href="#signIn" role="button" class="biliboard radial" data-toggle="modal">
					<button type="button" id="contentSignIn" class="btn btn-primary">SIGN IN</button>
				</a>
			</div>
			<div class="modal fade" id="signIn">
				<div class="modal-dialog">
					<div class="modal-content">
						<div class="modal-header">
							<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
							<h4 class="modal-title"> Facebook </h4>
						</div>
						<div class="modal-body text-center">
							<button type="button" class="btn btn-primary" id="sendButton"  style="font: 17px open, lato;"> Facebook </button>
						</div>
					</div>
				</div>
			</div>
		</div>
</div>
<!-- SCRIPTS -->
<script src="external/jquery/jquery.js"></script>
<script src="jquery-ui.js"></script>
</body>
</html>
